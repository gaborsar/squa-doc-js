{
  "private": true,
  "name": "squa-doc-js-packages",
  "license": "GNU LGPLv3",
  "scripts": {
    "benchmark": "babel-node benchmarks/document.js",
    "build": "webpack",
    "build:dev": "NODE_ENV=development npm run build",
    "format": "prettier --write \"packages/*/src/**/*.js\" \"benchmarks/**/*.js\" \"website/src/**/*.js\"",
    "jest": "jest",
    "coverage": "npm run jest -- --coverage",
    "lint": "eslint \"*.js\" \"packages/*/src/**/*.js\" \"benchmarks/**/*.js\" \"website/src/**/*.js\"",
    "test": "npm run lint && npm run jest",
    "postinstall": "npm run build",
    "start": "http-server website/public"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "1.1.8",
    "@fortawesome/fontawesome-svg-core": "^1.2.0",
    "@fortawesome/free-solid-svg-icons": "^5.1.0",
    "@fortawesome/react-fontawesome": "0.1.0",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.3",
    "babel-loader": "7.1.4",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "classnames": "2.2.6",
    "css-loader": "0.28.11",
    "enzyme": "3.3.0",
    "eslint": "4.19.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "7.9.1",
    "file-loader": "1.1.11",
    "http-server": "0.11.1",
    "jest": "23.1.0",
    "lodash": "4.17.10",
    "node-sass": "4.9.0",
    "prettier": "1.13.5",
    "quill-delta": "3.6.2",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "sass-loader": "7.0.3",
    "style-loader": "0.21.0",
    "webpack": "4.12.0",
    "webpack-cli": "^3.0.8"
  },
  "jest": {
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "unmockedModulePathPatterns": [
      "react",
      "enzyme",
      "quill-delta"
    ]
  }
}

{
    "private": true,
    "name": "squa-doc-js-packages",
    "license": "GNU LGPLv3",
    "scripts": {
        "benchmark": "babel-node benchmarks/document.js",
        "build": "webpack",
        "build:dev": "cross-env NODE_ENV=development npm run build",
        "format": "prettier --write \"packages/*/src/**/*.js\" \"benchmarks/**/*.js\" \"website/src/**/*.js\"",
        "jest": "jest",
        "coverage": "npm run jest -- --coverage",
        "lint": "eslint \"*.js\" \"packages/*/src/**/*.js\" \"benchmarks/**/*.js\" \"website/src/**/*.js\"",
        "test": "npm run lint && npm run jest",
        "postinstall": "npm run build",
        "start": "http-server website/public"
    },
    "dependencies": {
        "@babel/cli": "^7.2.3",
        "@babel/core": "^7.4.0",
        "@babel/plugin-proposal-class-properties": "^7.4.0",
        "@babel/polyfill": "^7.4.0",
        "@babel/preset-env": "^7.4.2",
        "@babel/preset-react": "^7.0.0",
        "@fortawesome/fontawesome": "^1.1.8",
        "@fortawesome/fontawesome-svg-core": "^1.2.17",
        "@fortawesome/free-solid-svg-icons": "^5.8.1",
        "@fortawesome/react-fontawesome": "^0.1.4",
        "babel-core": "^7.0.0-bridge.0",
        "babel-eslint": "^11.0.0-beta.0",
        "babel-jest": "^24.5.0",
        "babel-loader": "^8.0.5",
        "classnames": "^2.2.6",
        "css-loader": "^2.1.1",
        "eslint": "^5.15.3",
        "eslint-plugin-prettier": "^3.0.1",
        "eslint-plugin-react": "^7.12.4",
        "file-loader": "^3.0.1",
        "http-server": "^0.11.1",
        "jest": "^24.5.0",
        "node-sass": "^4.11.0",
        "prettier": "^1.16.4",
        "quill-delta": "^4.2.0",
        "react": "^16.8.5",
        "react-dom": "^16.8.5",
        "style-loader": "^0.23.1",
        "webpack": "^4.29.6",
        "webpack-cli": "^3.3.0"
    },
    "jest": {
        "unmockedModulePathPatterns": [
            "react",
            "enzyme",
            "quill-delta"
        ]
    },
    "devDependencies": {
        "@babel/node": "^7.2.2",
        "cross-env": "^5.2.0"
    }
}

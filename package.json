{
  "private": true,
  "name": "squa-doc-js-packages",
  "license": "GNU LGPLv3",
  "scripts": {
    "benchmark": "babel-node benchmarks/document.js",
    "build": "webpack",
    "build:dev": "NODE_ENV=development npm run build",
    "format": "prettier --write \"packages/*/src/**/*.js\" \"benchmarks/**/*.js\" \"website/src/**/*.js\"",
    "jest": "jest src",
    "lint": "eslint \"*.js\" \"packages/*/src/**/*.js\" \"benchmarks/**/*.js\" \"website/src/**/*.js\"",
    "test": "npm run lint && npm run jest",
    "postinstall": "npm run build",
    "start": "http-server website/public"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "1.1.5",
    "@fortawesome/fontawesome-free-regular": "5.0.10",
    "@fortawesome/fontawesome-free-solid": "5.0.10",
    "@fortawesome/react-fontawesome": "0.0.18",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.4",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "classnames": "2.2.5",
    "css-loader": "0.28.11",
    "file-loader": "1.1.11",
    "http-server": "0.11.1",
    "lodash": "4.17.5",
    "node-sass": "4.8.3",
    "quill-delta": "3.6.2",
    "react": "16.3.1",
    "react-dom": "16.3.1",
    "sass-loader": "6.0.7",
    "style-loader": "0.20.3",
    "webpack": "4.5.0"
  },
  "devDependencies": {
    "babel-eslint": "8.2.2",
    "enzyme": "3.3.0",
    "eslint-plugin-react": "7.7.0",
    "eslint": "4.19.1",
    "jest": "22.4.3",
    "prettier": "1.11.1"
  },
  "jest": {
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "unmockedModulePathPatterns": [
      "react",
      "enzyme",
      "quill-delta"
    ]
  }
}

{
  "name": "squa-doc-editor",
  "version": "2.0.1",
  "description": "Squa-Doc Editor",
  "main": "dist/SquaDocEditor.js",
  "scripts": {
    "lint": "eslint src",
    "jest": "jest src",
    "test": "npm run lint && npm run jest",
    "fmt:src": "prettier --write 'src/**/*.js'",
    "fmt:example": "prettier --write 'example/src/**/*.js'",
    "fmt": "npm run fmt:src && npm run fmt:example",
    "build": "webpack",
    "start": "node example/src/server.js",
    "postinstall": "npm run build"
  },
  "author": "Gabor Sar <gabor.sar@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.0",
    "babel-loader": "^7.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.24.0",
    "css-loader": "^0.28.0",
    "express": "^4.16.2",
    "lodash": "^4.17.4",
    "quill-delta": "^3.5.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "style-loader": "^0.18.0",
    "webpack": "^3.5.0"
  },
  "devDependencies": {
    "enzyme": "^3.0.0",
    "eslint": "^4.5.0",
    "eslint-plugin-react": "^7.0.0",
    "jest": "^21.1.0",
    "prettier": "^1.8.2"
  },
  "jest": {
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "unmockedModulePathPatterns": [
      "react",
      "enzyme",
      "quill-delta"
    ]
  }
}

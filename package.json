{
  "private": true,
  "name": "squa-doc-js-packages",
  "license": "GNU LGPLv3",
  "scripts": {
    "benchmark": "babel-node benchmarks/document.js",
    "build": "webpack",
    "build:dev": "NODE_ENV=development npm run build",
    "format": "prettier --write \"packages/*/src/**/*.js\" \"benchmarks/**/*.js\" \"website/src/**/*.js\"",
    "jest": "jest",
    "coverage": "npm run jest -- --coverage",
    "lint": "eslint \"*.js\" \"packages/*/src/**/*.js\" \"benchmarks/**/*.js\" \"website/src/**/*.js\"",
    "test": "npm run lint && npm run jest",
    "postinstall": "npm run build",
    "start": "http-server website/public"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "1.1.8",
    "@fortawesome/fontawesome-svg-core": "^1.2.10",
    "@fortawesome/free-solid-svg-icons": "^5.6.1",
    "@fortawesome/react-fontawesome": "0.1.3",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.4",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "classnames": "2.2.6",
    "css-loader": "2.0.1",
    "enzyme": "3.8.0",
    "eslint": "5.10.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "7.11.1",
    "file-loader": "2.0.0",
    "http-server": "0.11.1",
    "jest": "23.6.0",
    "lodash": "4.17.11",
    "node-sass": "4.11.0",
    "prettier": "1.15.3",
    "quill-delta": "4.1.0",
    "react": "16.6.3",
    "react-dom": "16.6.3",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "webpack": "4.27.1",
    "webpack-cli": "^3.1.2"
  },
  "jest": {
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "unmockedModulePathPatterns": [
      "react",
      "enzyme",
      "quill-delta"
    ]
  }
}

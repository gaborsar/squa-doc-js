{
    "private": true,
    "name": "squa-doc-js-packages",
    "license": "GNU LGPLv3",
    "scripts": {
        "benchmark": "babel-node benchmarks/document.js",
        "build": "webpack",
        "build:dev": "cross-env NODE_ENV=development npm run build",
        "format": "prettier --write \"packages/*/src/**/*.js\" \"benchmarks/**/*.js\" \"website/src/**/*.js\"",
        "jest": "jest",
        "coverage": "npm run jest -- --coverage",
        "lint": "eslint \"*.js\" \"packages/*/src/**/*.js\" \"benchmarks/**/*.js\" \"website/src/**/*.js\"",
        "test": "npm run lint && npm run jest",
        "postinstall": "npm run build",
        "start": "http-server website/public"
    },
    "dependencies": {
        "@babel/cli": "^7.2.0",
        "@babel/core": "^7.2.2",
        "@babel/plugin-proposal-class-properties": "^7.2.1",
        "@babel/polyfill": "^7.0.0",
        "@babel/preset-env": "^7.2.0",
        "@babel/preset-react": "^7.0.0",
        "@fortawesome/fontawesome": "^1.1.8",
        "@fortawesome/fontawesome-svg-core": "^1.2.10",
        "@fortawesome/free-solid-svg-icons": "^5.6.1",
        "@fortawesome/react-fontawesome": "^0.1.3",
        "babel-core": "^7.0.0-bridge.0",
        "babel-eslint": "^10.0.1",
        "babel-jest": "^23.6.0",
        "babel-loader": "^8.0.4",
        "classnames": "^2.2.6",
        "css-loader": "^2.0.1",
        "eslint": "^5.10.0",
        "eslint-plugin-prettier": "^3.0.0",
        "eslint-plugin-react": "^7.11.1",
        "file-loader": "^2.0.0",
        "http-server": "^0.11.1",
        "jest": "^23.6.0",
        "node-sass": "^4.11.0",
        "prettier": "^1.15.3",
        "quill-delta": "^4.1.0",
        "react": "^16.6.3",
        "react-dom": "^16.6.3",
        "style-loader": "^0.23.1",
        "webpack": "^4.27.1",
        "webpack-cli": "^3.1.2"
    },
    "jest": {
        "unmockedModulePathPatterns": [
            "react",
            "enzyme",
            "quill-delta"
        ]
    },
    "devDependencies": {
        "@babel/node": "^7.2.2",
        "cross-env": "^5.2.0"
    }
}
